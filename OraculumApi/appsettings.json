{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowAnonymous": false,
  "DBSibyllaeConfigOnly": true,
  "Tenants": {
    "Default": {
      "Weaviate": {
        "ServiceEndpoint": "your-weaviate-service-endpoint-here"
      },
      "OpenAI": {
        "ApiKey": "your-api-key-here",
        "OrgId": "your-org-id-here"
      },
      "Security": {
        "OIDC": {
          "Authority": "your-oidc-authority-url-here",
          "ClientId": "your-client-id-here",
          "Audience": "your-valid-audience-here"
        },
        "AuthorizationRolesMap": {
          "sysadmin": [
            "sysadmin"
          ],
          "backoffice": [
            "backoffice"
          ],
          "frontoffice": [
            "frontoffice"
          ]
        }
      }
    }
  },
  "SibyllaConf": "Demo",
  "DataIngestion": {
    "TimeoutMinutes": 10
  }
}