{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Weaviate": {
    "ServiceEndpoint": "your-weaviate-service-endpoint-here"
  },
  "OpenAI": {
    "ApiKey": "your-api-key-here",
    "OrgId": "your-org-id-here"
  },
  "SibyllaConf": "Demo",
  "AuthorizationRolesMap": {
    "SysAdmin": [
      "sysadmin"
    ],
    "BackOffice": [
      "backoffice"
    ],
    "FrontOffice": [
      "frontoffice"
    ]
  },
  "AllowAnonymous": false,
  "AnonymousRoles": [
    "FrontOffice"
  ],
  "OIDC": {
    "Authority": "your-oidc-authority-here",
    "ClientId": "your-client-id-here",
    "ClientSecret": "your-client-secret-here"
  }
}